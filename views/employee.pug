extends layout

block content
  h1= title
  - var results=res;
  

  div(class="container-fluid")
    div(class="row")
      each item, key in results
        - var value=item;
        - var index=key;
        div(class="col-sm-2", onclick=`buy(${index})`) name is #{item.name} <br /> age is #{item.age} 

  script.
   function buy(x) 
   {
     alert(`You purchased item ${x}`)
   } 
      
    
